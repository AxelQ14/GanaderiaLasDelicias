@model GanaderiaLasDelicias.Models.Pregnancy

@{
    ViewData["Title"] = "Delete";
}

<div>
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Eliminar Embarazo</h3>
        </div>
        <div ="card-body">

            <hr />
            <dl class="row" style="margin-left: 1%;">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PregDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PregDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Note)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Note)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Bull)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bull.BullId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cow)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cow.CowId)
        </dd>
        </div>
    </div>
    <div>
        <form asp-action="Delete">
            <input type="hidden" asp-for="PregnancyId" />
            <input type="submit" value="Desactivar" class="btn btn-danger" /> |
            <a asp-action="Index">Regresar</a>
        </form>
    </div>
