@model GanaderiaLasDelicias.Models.Pregnancy

@{
    ViewData["Title"] = "Edit";
}

<div class="card">
    <div class="card-header">
        <h3>Editar Embarazo</h3>
    </div>


    <hr />
    <div class="row" style="margin-left: 1%;">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PregnancyId" />
            <div class="form-group">
                <label asp-for="CowId" class="control-label"></label>
                <select asp-for="CowId" class="form-control" asp-items="ViewBag.CowId"></select>
                <span asp-validation-for="CowId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PregDate" class="control-label"></label>
                <input asp-for="PregDate" class="form-control" />
                <span asp-validation-for="PregDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BullId" class="control-label"></label>
                <select asp-for="BullId" class="form-control" asp-items="ViewBag.BullId"></select>
                <span asp-validation-for="BullId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Note" class="control-label"></label>
                <input asp-for="Note" class="form-control" />
                <span asp-validation-for="Note" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>
